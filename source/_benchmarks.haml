- data.benchmarks.each do |(data_set, values)|
  %div{:class => "panel panel-default"}
    .panel-heading
      %b= 'Read Set ' + data_set
      %p
        %small= data_description data_set
    %table{:class => "table table-condensed tablesorter score"}
      %thead
        %tr
          %th{:class => "text-left col-md-3"}
            Assembler Image
          %th{:class => "text-left col-md-2"}
            Procfile
          %th{:class => "text-right col-md-1"}
            %span{"data-toggle"    => "tooltip",
                  "data-placement" => "top",
                  "title"          => "NG50"}
              NG50
            %span{:class => "glyphicon glyphicon-sort"}
          %th{:class => "text-right col-md-1"}
            %span{"data-toggle"    => "tooltip",
                  "data-placement" => "top",
                  "title"          => "LG50"}
              LG50
            %span{:class => "glyphicon glyphicon-sort"}
          %th{:class => "text-right col-md-1"}
            %span{"data-toggle"    => "tooltip",
                  "data-placement" => "top",
                  "title"          => "Percentage of reference genome unassembled."}
              UN
            %span{:class => "glyphicon glyphicon-sort"}
          %th{:class => "text-right col-md-1"}
            %span{"data-toggle"    => "tooltip",
                  "data-placement" => "top",
                  "title"          => "Number of incorrect, indels, and ambiguous bases per 100kbp."}
              IB
            %span{:class => "glyphicon glyphicon-sort"}
      %tbody
        - values.select{|i| i[:ng50]}.each do |value|
          %tr
            %td.text-left= value.image
            %td.text-left= value.proc
            %td.text-right= value.ng50
            %td.text-right= value.lg50
            %td.text-right= value.genome_fraction
            %td.text-right= value.incorrect
