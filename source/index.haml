- data.results.each do |data_set|
  %div{:class => "panel panel-default"}
    .panel-heading
      %b= 'Read Set ' + data_set.name
      %p
        %small= data_description data_set.name
    %table{:class => "table table-condensed tablesorter score"}
      %thead
        %tr
          %th{:class => "text-left col-md-4"}
          %th{:class => "text-right col-md-2"}
            %span{"data-toggle"    => "tooltip",
                  "data-placement" => "top",
                  "title"          => "Contigs over 1000bp in length."}
              Contigs
            %span{:class => "glyphicon glyphicon-sort"}
          %th{:class => "text-right col-md-2"}
            %span{"data-toggle"    => "tooltip",
                  "data-placement" => "top",
                  "title"          => "Percentage of reference genome unassembled."}
              Unassembled
            %span{:class => "glyphicon glyphicon-sort"}
          %th{:class => "text-right col-md-2"}
            %span{"data-toggle"    => "tooltip",
                  "data-placement" => "top",
                  "title"          => "Number of incorrectly assembled bases per 100kbp."}
              Incorrect
            %span{:class => "glyphicon glyphicon-sort"}
          %th{:class => "text-right col-md-2"}
            %span{"data-toggle"    => "tooltip",
                  "data-placement" => "top",
                  "title"          => "Number of incorrectly inserted and deleted bases per 100kbp."}
              Indels
            %span{:class => "glyphicon glyphicon-sort"}
      %tbody
        - data_set.values.last.each do |value|
          %tr
            %td.text-left= value.namespace + '/' + value.tool
            %td.text-right= value.n_contigs_gt_1000.to_i
            %td.text-right= value.genome_fraction
            %td.text-right= value.mismatches_per_100k
            %td.text-right= value.indels_per_100k
