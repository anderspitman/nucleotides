- data.results.each do |data_set|
  %div{:class => "panel panel-default"}
    .panel-heading
      %b= 'Read Set ' + data_set.name
    %table{:class => "table table-condensed tablesorter", :id => 'score'}
      %thead
        %tr
          %th{:class => "text-left col-md-2"}
          %th{:class => "text-right col-md-2"}
            %p
              %span{:class => "glyphicon glyphicon-sort"}
              Contigs (> 1000bp)
          %th{:class => "text-right col-md-2"}
            %p
              %span{:class => "glyphicon glyphicon-sort"}
              Unassembled (%)
          %th{:class => "text-right col-md-2"}
            %p
              %span{:class => "glyphicon glyphicon-sort"}
              Incorrect (per 100kbp)
          %th{:class => "text-right col-md-2"}
            %p
              %span{:class => "glyphicon glyphicon-sort"}
              Indels (per 100kbp)
      %tbody
        - data_set.values.last.each do |value|
          %tr
            %td.text-left= value.namespace + '/' + value.tool
            %td.text-right= value.n_contigs_gt_1000.to_i
            %td.text-right= value.genome_fraction
            %td.text-right= value.mismatches_per_100k
            %td.text-right= value.indels_per_100k
